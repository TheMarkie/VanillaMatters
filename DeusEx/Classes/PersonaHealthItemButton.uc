//=============================================================================
// PersonaHealthItemButton
//=============================================================================
class PersonaHealthItemButton extends PersonaItemButton;

var int    partIndex;
var String desc;
var String title;

// ----------------------------------------------------------------------
// SetPartIndex()
// ----------------------------------------------------------------------

function SetPartIndex(int newPartIndex)
{
    partIndex = newPartIndex;
}

// ----------------------------------------------------------------------
// GetPartIndex()
// ----------------------------------------------------------------------

function int GetPartIndex()
{
    return partIndex;
}

// ----------------------------------------------------------------------
// SetDesc()
// ----------------------------------------------------------------------

function SetDesc(String newDesc)
{
    desc = newDesc;
}

// ----------------------------------------------------------------------
// GetDesc()
// ----------------------------------------------------------------------

function String GetDesc()
{
    return desc;
}

// ----------------------------------------------------------------------
// SetTitle()
// ----------------------------------------------------------------------

function SetTitle(String newTitle)
{
    title = newTitle;
}

// ----------------------------------------------------------------------
// GetTitle()
// ----------------------------------------------------------------------

function String GetTitle()
{
    return title;
}

